                "selling",
                "procurement",
                "import",
                "export",
                "shipping",
                "delivery",
                "minimum order",
                "payment terms"
            ],
            "regex": [
                "\\bprice\\b",
                "\\bcost\\b",
                "\\brfq\\b",
                "\\bmoq\\b",
                "\\bbuy\\b",
                "\\bsupplier(s)?\\b",
                "\\bmanufacturer(s)?\\b",
                "\\bdealer(s)?\\b"
