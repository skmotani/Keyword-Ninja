            "weight": 3,
            "description": "Commercial/Purchase intent - looking to buy, get quotes, find suppliers",
            "phrases": [
                "price",
                "cost",
                "rate",
                "quotation",
                "quote",
                "for sale",
                "order",
                "purchase",
                "deal",
                "offer",
                "supplier",
                "manufacturer",
                "exporter",
                "distributor",
                "vendor",
                "wholesaler",
                "bulk",
                "wholesale",
                "factory price",
                "moq",
                "rfq",
