    "bucket_priors": {
        "include": {
            "transactional": 0.5,
            "commercial_investigation": 0.3
        },
        "review": {
            "informational": 0.5,
            "commercial_investigation": 0.3
        },
        "brand": {
            "navigational": 0.7
        },
        "exclude": {
            "noise": 0.8
